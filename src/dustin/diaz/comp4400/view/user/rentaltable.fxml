<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="borderPane" minHeight="900.0" minWidth="1011.0"
            prefHeight="900.0" prefWidth="1011.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="dustin.diaz.comp4400.controller.RentalTableController">
    <center>
        <VBox alignment="CENTER" minHeight="800.0" minWidth="1000.0" BorderPane.alignment="CENTER">
            <opaqueInsets>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </opaqueInsets>
            <BorderPane.margin>
                <Insets bottom="50.0" left="50.0" right="50.0" top="50.0"/>
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <children>
                <HBox alignment="CENTER_LEFT" VBox.vgrow="ALWAYS">
                    <children>
                        <Button fx:id="backBtn" mnemonicParsing="false" prefWidth="110.0" text="Home Screen">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </Button>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="701.0" prefWidth="980.0">
                    <children>
                        <TableView fx:id="tableView" editable="true" prefHeight="658.0" prefWidth="948.0"
                                   tableMenuButtonVisible="true" HBox.hgrow="ALWAYS">
                            <columns>
                                <TableColumn prefWidth="75.0" text="C1"/>
                                <TableColumn prefWidth="75.0" text="C2"/>
                            </columns>
                        </TableView>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" VBox.vgrow="ALWAYS">
                    <children>
                        <Button fx:id="deleteButton" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0"
                                text="Return Rental">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="addBtn" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0"
                                text="Add Rental">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </Button>
                        <Label fx:id="warning" textFill="RED"/>
                        <Button fx:id="refreshBtn" mnemonicParsing="false" prefWidth="110.0" text="Refresh View">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>
</BorderPane>
